  "keywords": ["data", "json", "xml", "processor", "popo"],
  "homepage": "https://phpsx.org",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Christoph Kappestein",
      "email": "christoph.kappestein@gmail.com",
      "homepage": "https://chrisk.app"
    }
  ],
  "require": {
    "php": ">=8.0",
    "psx/datetime": "^2.0",
    "psx/http": "^3.0",
    "psx/json": "^2.0",
    "psx/model": "^4.0",
    "psx/record": "^2.0",
    "psx/schema": "^4.0",
    "psx/validate": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "vimeo/psalm": "^4.0"
